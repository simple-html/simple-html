    "autoprefixer": "^9.7.4",
    "custom-elements-hmr-polyfill": "^1.0.0",
    "fuse-box": "^4.0.0-next.407",
    "fuse-box-typechecker": "^3.0.0-next.31",
    "lit-html": "^1.1.2",
    "marked": "^0.8.0",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.2.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
